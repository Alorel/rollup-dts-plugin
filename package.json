{
  "name": "@alorel/rollup-plugin-dts",
  "version": "2.0.0",
  "description": "Generates .d.ts output",
  "main": "index.js",
  "repository": "https://github.com/Alorel/rollup-dts-plugin.git",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "author": "Arturas Molcanovas <a.molcanovas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "doublebuild": "rollup -c && cross-env DTS_RUN_DIST=1 rollup -c",
    "rollup": "rollup -c",
    "release": "semantic-release",
    "lint": "tslint -s node_modules/custom-tslint-formatters/formatters -t grouped -p .",
    "lint:fix": "yarn run lint --fix",
    "try-tsc": "tsc -p . --declaration --emitDeclarationOnly --outDir dist/potat",
    "rollup:watch": "yarn run rollup --watch"
  },
  "peerDependencies": {
    "rollup": "^2.6.0 || ^1.30.0",
    "typescript": "^3.5.0"
  },
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.1.3",
    "@alorel-personal/tslint-rules": "^5.0.7",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@types/bluebird": "^3.5.32",
    "@types/cross-spawn": "^6.0.2",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.11.1",
    "@types/tmp": "^0.2.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.3",
    "custom-tslint-formatters": "^2.4.0",
    "del": "^5.1.0",
    "rollup": "^2.27.1",
    "semantic-release": "^17.1.2",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/alorel"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "cross-spawn": "^7.0.3",
    "glob": "^7.1.6",
    "tmp": "^0.2.1"
  }
}
